{
  "name": "@midday/supabase",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "lint": "biome check .",
    "format": "biome format --write .",
    "dev": "supabase start",
    "typecheck": "tsc --noEmit",
    "login": "supabase login",
    "db:reset": "",
    "db:generate": "supabase gen types typescript --project-id $PROJECT_ID --schema public > src/types/db.ts"
  },
  "dependencies": {
    "@supabase/ssr": "^0.3.0",
    "react": "^18.3.1",
    "server-only": "^0.0.1",
    "supabase": "^1.178.2"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.43.5",
    "@types/pg": "^8.11.6",
    "typescript": "^5.5.2"
  },
  "exports": {
    "./server": "./src/client/server.ts",
    "./client": "./src/client/client.ts",
    "./mutations": "./src/mutations/index.ts",
    "./middleware": "./src/client/middleware.ts",
    "./queries": "./src/queries/index.ts",
    "./cached-queries": "./src/queries/cached-queries.ts",
    "./storage": "./src/utils/storage.ts",
    "./types": "./src/types/index.ts"
  }
}
