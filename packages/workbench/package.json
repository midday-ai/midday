{
  "name": "workbench",
  "version": "0.1.0",
  "description": "A modern, Trigger.dev-inspired job queue dashboard for BullMQ",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./hono": {
      "types": "./dist/adapters/hono.d.ts",
      "import": "./dist/adapters/hono.js"
    },
    "./express": {
      "types": "./dist/adapters/express.d.ts",
      "import": "./dist/adapters/express.js"
    },
    "./fastify": {
      "types": "./dist/adapters/fastify.d.ts",
      "import": "./dist/adapters/fastify.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:ui && npm run build:lib",
    "build:lib": "tsc",
    "build:ui": "vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write ."
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@types/dagre": "^0.7.53",
    "@xyflow/react": "^12.10.0",
    "dagre": "^0.8.5",
    "framer-motion": "^12.34.0",
    "lru-cache": "^11.2.4"
  },
  "devDependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-router": "^1.147.3",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.3",
    "autoprefixer": "^10.4.23",
    "bullmq": "catalog:",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "1.1.1",
    "date-fns": "catalog:",
    "hono": "catalog:",
    "ioredis": "^5.9.1",
    "lucide-react": "^0.562.0",
    "postcss": "^8.5.6",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.6.0",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "catalog:",
    "vite": "^7.3.1",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "bullmq": ">=5.0.0",
