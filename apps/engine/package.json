    "plaid": "^38.1.0",
    "typesense": "^2.1.0",
    "workers-ai-provider": "^2.0.0",
    "xior": "^0.7.8",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250904.0",
    "wrangler": "^4.33.2"
  },
  "exports": {
