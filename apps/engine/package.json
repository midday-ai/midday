{
  "name": "@midday/engine",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev": "NODE_ENV=development wrangler dev src/index.ts --port 3002",
    "deploy": "wrangler deploy --minify src/index.ts",
    "clean": "rm -rf .turbo node_modules",
    "lint": "biome check .",
    "test": "bun test src",
    "lint:fix": "biome format --write .",
    "typecheck": "tsc --noEmit",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "build": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@hono/zod-openapi": "1.2.0",
    "@hono/zod-validator": "^0.7.6",
    "hono": "^4.11.5",
    "jose": "^6.1.3",
    "plaid": "^41.0.0",
    "typesense": "^2.1.0",
    "workers-ai-provider": "^3.0.2",
    "xior": "^0.8.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260115.0",
    "wrangler": "^4.59.2"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./account": "./src/utils/account.ts",
    "./gocardless/utils": "./src/providers/gocardless/utils.ts"
  }
}
